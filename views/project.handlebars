<div class="container container-fluid">
  <div class="card" style="width: 18rem;">
    <div class="card-body">
      <h5 class="card-title">{{project.name}}</h5>
      <h6 class="card-subtitle mb-2 text-muted">{{project.date_created}}</h6>
      <p class="card-text">{{project.description}}</p>
      <p class="card-text">Needed funding: ${{project.needed_funding}}</p>
    </div>
    <button class="btn btn-primary">FUND NOW!</button>
  </div>
</div>